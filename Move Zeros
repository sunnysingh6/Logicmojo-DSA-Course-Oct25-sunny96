public static List<Integer> move_func(int n, List<Integer> arr) {
    // Write your code here
        int i=0,j=0;
        while(j<n){
            if(arr.get(j)!=0){
                int temp=arr.get(i);
                arr.set(i,arr.get(j));
                arr.set(j,temp);
                i++;
            }
            j++;
        }
        
        return arr;
        }
